[ignore]
.*/build/.*
.*/react/node_modules/.*
.*/node_modules/babel-helper-remap-async-to-generator/lib/.*
.*/node_modules/babel-helper-builder-binary-assignment-operator-visitor/lib/.*
.*/node_modules/babel-helper-explode-assignable-expression/lib/.*
.*/node_modules/fbjs/*
.*/node_modules/airflux/es6/*

[include]
./src
./test
./node_modules/react
./node_modules/airflux/lib

[libs]

[options]
strip_root=true
munge_underscores=true
unsafe.enable_getters_and_setters=true
esproposal.decorators=ignore
esproposal.class_static_fields=ignore
esproposal.class_instance_fields=ignore

suppress_comment= \\(.\\|\n\\)*\\$IgnoreFlow
suppress_comment= \\(.\\|\n\\)*\\$FlowPrototypeDecl
suppress_comment= \\(.\\|\n\\)*\\$FlowComputedProperty
suppress_comment= \\(.\\|\n\\)*\\$FlowAnnotation
suppress_comment= \\(.\\|\n\\)*\\$ScssImport
